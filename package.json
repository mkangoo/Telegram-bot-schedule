{
	"author": "mkangoo",
	"dependencies": {
		"dotenv": "^16.4.2",
		"nodemon": "^3.0.3",
		"telegraf": "^4.15.3"
	},
	"description": "Bot for viewing the schedule",
	"devDependencies": {
		"@babel/core": "^7.23.9",
		"@babel/preset-env": "^7.23.9",
		"@vitest/coverage-istanbul": "^1.3.1",
		"babel-jest": "^29.7.0",
		"eslint": "^8.56.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"husky": "^9.0.10",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"vite": "^5.1.5",
		"vitest": "^1.3.1"
	},
	"keywords": [],
	"license": "MIT",
	"lint-staged": {
		"*.js": [
			"npm run lint:fix",
			"git add"
		]
	},
	"main": "index.js",
	"name": "bot-schedule-guu",
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": true,
		"endOfLine": "lf",
		"jsxSingleQuote": false,
		"printWidth": 140,
		"semi": false,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mkangoo/Telegram-bot-schedule"
	},
	"scripts": {
		"coverage": "vitest run --coverage",
		"dev": "nodemon src/index.js",
		"lint": "eslint ./src",
		"lint:fix": "eslint ./src --fix --color",
		"prepare": "husky",
		"start": "node src/index.js",
		"test": "vitest --run"
	},
	"type": "module",
	"version": "1.0.3"
}
